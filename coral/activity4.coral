float celsiusValue
float fahrenheitValue
integer userChoice

userChoice = 1
celsiusValue = 0.0

while userChoice == 1
   fahrenheitValue = (celsiusValue * 9.0 / 5.0) + 32.0
   Put celsiusValue to output
   Put "C is " to output
   Put fahrenheitValue to output
   Put "F\n" to output
   celsiusValue = celsiusValue + 5
   userChoice = Get next input

Put "Goodbye." to output